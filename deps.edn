{:paths ["src" "resources"]
 :deps
 {org.clojure/clojure {:mvn/version "1.9.0"}

  ;; For use with Spec
  org.clojure/test.check {:mvn/version "0.9.0"}
  expound                {:mvn/version "0.3.1"}

  buddy/buddy-auth         {:mvn/version "2.1.0"}
  integrant                {:mvn/version "0.6.3"}
  clj-time                 {:mvn/version "0.14.2"}
  com.datomic/datomic-free {:mvn/version "0.9.5656" :exclusions
                            [org.slf4j/slf4j-log4j12
                             org.slf4j/slf4j-nop]}

  org.mindrot/jbcrypt            {:mvn/version "0.4"}
  ch.qos.logback/logback-classic {:mvn/version "1.1.7" :exclusions
                                  [org.slf4j/slf4j-api]}
  org.clojure/tools.logging      {:mvn/version "0.4.0"}

  ;; Web
  ring/ring-jetty-adapter        {:mvn/version "1.6.3"}
  org.eclipse.jetty/jetty-server {:mvn/version "9.3.7.v20160115"}
  ataraxy                        {:mvn/version "0.4.0"}
  hiccup                         {:mvn/version "1.0.5"}

  ;; Cljs
  org.clojure/clojurescript {:mvn/version "1.9.946"}
  com.cemerick/piggieback   {:mvn/version "0.2.2" :exclusions
                             [com.google.javascript/closure-compiler]}
  figwheel-sidecar          {:mvn/version "0.5.14" :exclusions
                             [com.google.javascript/closure-compiler]}
  rum                       {:mvn/version "0.11.1"}

  ;; REPL
  org.clojure/tools.nrepl {:mvn/version "0.2.12"}
  cider/cider-nrepl       {:mvn/version "0.16.0"}
  refactor-nrepl          {:mvn/version "2.3.1"}}

 :aliases
 {:dev
  {:jvm-opts    ["-Dlogback.configurationFile=logback.xml"]
   :extra-paths ["dev"]
   :extra-deps
   {integrant/repl     {:mvn/version "0.3.0"}
    com.taoensso/tufte {:mvn/version "1.1.2"}}}

  :test
  {:extra-paths ["test"]
   :extra-deps
   {com.cognitect/test-runner
    {:git/url "git@github.com:cognitect-labs/test-runner"
     :sha     "5fb4fc46ad0bf2e0ce45eba5b9117a2e89166479"}}
   :main-opts   ["-m" "cognitect.test-runner"]}

  :pack
  {:extra-deps
   {pack/pack.alpha
    {:git/url "git@github.com:juxt/pack.alpha.git"
     :sha     "eed89476315661e2389d5d078ec17375bec3efa5"}}
   :main-opts ["-m" "mach.pack.alpha.jcl" "deps.edn" "uberjar.jar" "resources"]}}}
